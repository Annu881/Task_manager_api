<?xml version="1.0" encoding="UTF-8"?>
<mxfile host="app.diagrams.net">
  <diagram name="ERD">
    <mxGraphModel dx="1000" dy="1000" grid="1" gridSize="10" guides="1">
      <root>
        <!-- required root cells -->
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>

        <!-- USER -->
        <mxCell id="user" value="User&#10;-------&#10;id (PK)&#10;name&#10;email&#10;password"
                style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="40" y="40" width="180" height="160" as="geometry"/>
        </mxCell>

        <!-- TASK -->
        <mxCell id="task" value="Task&#10;-------&#10;id (PK)&#10;title&#10;description&#10;status&#10;created_at&#10;updated_at&#10;user_id (FK)"
                style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="300" y="40" width="180" height="180" as="geometry"/>
        </mxCell>

        <!-- LABEL -->
        <mxCell id="label" value="Label&#10;-------&#10;id (PK)&#10;name&#10;color"
                style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="560" y="40" width="180" height="160" as="geometry"/>
        </mxCell>

        <!-- COMMENT -->
        <mxCell id="comment" value="Comment&#10;-------&#10;id (PK)&#10;text&#10;created_at&#10;task_id (FK)&#10;user_id (FK)"
                style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="300" y="260" width="180" height="160" as="geometry"/>
        </mxCell>

        <!-- TASKLABEL -->
        <mxCell id="tasklabel" value="TaskLabel&#10;-------&#10;task_id (FK)&#10;label_id (FK)"
                style="rounded=1;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="560" y="260" width="180" height="160" as="geometry"/>
        </mxCell>

        <!-- RELATIONSHIPS -->
        <mxCell id="r1" edge="1" source="user" target="task" style="endArrow=block;html=1;" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="r2" edge="1" source="task" target="comment" style="endArrow=block;html=1;" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="r3" edge="1" source="task" target="tasklabel" style="endArrow=block;html=1;" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="r4" edge="1" source="label" target="tasklabel" style="endArrow=block;html=1;" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="r5" edge="1" source="user" target="comment" style="endArrow=block;html=1;" parent="1">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
